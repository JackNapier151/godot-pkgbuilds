package_godot-dev-mono-bin(){
	pkgdesc="${pkgdescalt} Dev Mono Binary Version"
	depends+=("${_monodepends}")
	groups+=(
		"godot-mono"
		"godot-standard"
		"godot-standard64"
		"godot-64"
	)
	install -Dm755 "${srcdir}/Godot_v${_pkgver}-dev${_pkgverb}_mono_linux_x86_64/Godot_v${_pkgver}-dev${_pkgverb}_mono_linux.x86_64" "${pkgdir}/opt/godot/mono/godot/dev/bin/desktop/godot-dev-mono-bin"
	cp -r "${srcdir}/Godot_v${_pkgver}-dev${_pkgverb}_mono_linux_x86_64/GodotSharp" "${pkgdir}/opt/godot/mono/godot/dev/bin/desktop/GodotSharp"
	ln -s "/opt/godot/mono/godot/dev/bin/desktop/godot-dev-mono-bin" "${pkgdir}/opt/godot/godot-dev-mono-bin"
	
	install -d "${pkgdir}/usr/bin/"
	ln -s "/opt/godot/mono/godot/dev/bin/desktop/godot-dev-mono-bin" "${pkgdir}/usr/bin/godot-dev-mono-bin"
	
	install -d "${pkgdir}/usr/share/applications/"
	cat > "${pkgdir}/usr/share/applications/godot-dev-mono-bin.desktop" <<-EOF
		[Desktop Entry]
		Name=Godot Engine Dev Mono Binary v${_pkgver}-dev${_pkgverb}
		GenericName="Advanced cross-platform 2D and 3D libre game engine. Dev Mono Binary Version"
		Comment=Application for making games
		Exec=/usr/bin/godot-dev-mono-bin %U
		Icon=godot
		Terminal=false
		Type=Application
		MimeType=application/x-godot-project;
		Categories=Development;IDE;
	EOF
}

