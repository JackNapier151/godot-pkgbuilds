	"godot4-rc-mono-win32-bin"
	"godot4-rc-mono-win64-bin"
	)
_pkgver="4.2"
_pkgverb=2
pkgver="${_pkgver}rc${_pkgverb}"
pkgrel=1
pkgdesc="Godot4 Game Engine: An advanced, feature packed, multi-platform 2D and 3D game engine."
url="http://www.godotengine.org"
license=("MIT")
groups=(
	"godot-full"
	"godot4"
	"godot42"
	"godot-bin"
	"godot-rc"
)
arch=("any")
_monodepends=(
	"mono>=6.12.0"
	"dotnet-sdk"
	"msbuild"
)
