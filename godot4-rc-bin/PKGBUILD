pkgname=(
	"godot4-rc-src"
	##################
	# Godot GDScript #
	##################
	"godot4-rc-bin"
	"godot4-rc-bin32"
	"godot4-rc-export-templates-bin"
	"libgodot4-rc-bin"
	"godot4-rc-web-bin"
	"godot4-rc-osx-bin"
	"godot4-rc-win32-bin"
	"godot4-rc-win64-bin"
	"godot4-rc-android-bin"
	##############
