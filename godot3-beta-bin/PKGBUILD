	cp -r "${srcdir}/Godot_v${_pkgver}-beta${_pkgverb}_mono_x11_64/GodotSharp" "${pkgdir}/opt/godot/mono/godot3/beta/bin/desktop/GodotSharp"
	ln -s "/opt/godot/mono/godot3/beta/bin/desktop/godot3-beta-mono-bin" "${pkgdir}/opt/godot/godot3-beta-mono-bin"
	
	install -d "${pkgdir}/usr/bin/"
	ln -s "/opt/godot/mono/godot3/beta/bin/desktop/godot3-beta-mono-bin" "${pkgdir}/usr/bin/godot3-beta-mono-bin"
	
	install -d "${pkgdir}/usr/share/applications/"
	cat > "${pkgdir}/usr/share/applications/godot3-beta-mono-bin.desktop" <<-EOF
		[Desktop Entry]
		Name=Godot3 Engine Beta Mono Binary v${_pkgver}-beta${_pkgverb}
