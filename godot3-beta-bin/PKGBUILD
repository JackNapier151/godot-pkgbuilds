package_godotlts-beta-bin(){
	pkgdesc+="${pkgdescalt} LTS Beta Binary Version"
	groups+=(
		"godot-lts"
		"godot-gdscript"
		"godot-standard"
		"godot-standard64"
		"godot-64"
	)
	install -Dm755 "${srcdir}/Godot_v${_pkgver}-beta${_pkgverb}_x11.64" "${pkgdir}/opt/godot/godotlts-beta-bin"
	
	install -d "${pkgdir}/usr/bin/"
	ln -s "/opt/godot/godotlts-beta-bin" "${pkgdir}/usr/bin/godotlts-beta-bin"
	
	install -d "${pkgdir}/usr/share/applications/"
	cat > "${pkgdir}/usr/share/applications/godotlts-beta-bin.desktop" <<-EOF
		[Desktop Entry]
		Name=Godot LTS Engine Beta Binary v${_pkgver}-beta${_pkgverb}
		GenericName="Advanced cross-platform 2D and 3D libre game engine. LTS Beta Binary Version"
		Comment=Application for making games
		Exec=/usr/bin/godotlts-beta-bin %U
		Icon=godot
		Terminal=false
		Type=Application
		MimeType=application/x-godot-project;
		Categories=Development;IDE;
	EOF
}

