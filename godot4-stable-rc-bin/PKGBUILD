package_godot-stable-rc-mono-bin(){
	pkgdesc+="${pkgdescalt} Stable Release Candidate Mono Binary Version"
	depends+=("${_monodepends}")
	groups+=(
		"godot-mono"
		"godot-standard"
		"godot-standard64"
		"godot-64"
	)
	install -Dm755 "${srcdir}/Godot_v${_pkgver}-rc${_pkgverb}_mono_linux_x86_64/Godot_v${_pkgver}-rc${_pkgverb}_mono_linux.x86_64" "${pkgdir}/opt/godot/mono/godot/stable-rc/bin/desktop/godot-stable-rc-mono-bin"
	cp -r "${srcdir}/Godot_v${_pkgver}-rc${_pkgverb}_mono_linux_x86_64/GodotSharp" "${pkgdir}/opt/godot/mono/godot/stable-rc/bin/desktop/GodotSharp"
	ln -s "/opt/godot/mono/godot/stable-rc/bin/desktop/godot-stable-rc-mono-bin" "${pkgdir}/opt/godot/godot-stable-rc-mono-bin"
	
	install -d "${pkgdir}/usr/bin/"
	ln -s "/opt/godot/mono/godot/stable-rc/bin/desktop/godot-stable-rc-mono-bin" "${pkgdir}/usr/bin/godot-stable-rc-mono-bin"
	
	install -d "${pkgdir}/usr/share/applications/"
	cat > "${pkgdir}/usr/share/applications/godot-stable-rc-mono-bin.desktop" <<-EOF
		[Desktop Entry]
		Name=Godot Engine Stable Release Candidate Mono Binary v${_pkgver}-rc${_pkgverb}
		GenericName="Advanced cross-platform 2D and 3D libre game engine. Stable Release Candidate Mono Binary Version"
		Comment=Application for making games
		Exec=/usr/bin/godot-stable-rc-mono-bin %U
		Icon=godot
		Terminal=false
		Type=Application
		MimeType=application/x-godot-project;
		Categories=Development;IDE;
	EOF
}

