	groups+=(
		"godot-gdscript"
		"godot-win"
		"godot-win32"
		"godot-32"
	)
	install -Dm755 "${srcdir}/Godot_v${_pkgver}-rc${_pkgverb}_win32.exe" "${pkgdir}/opt/godot/godot-stable-rc-win32-bin.exe"
	#install -Dm755 "${srcdir}/Godot_v${_pkgver}-rc${_pkgverb}_win32_console.cmd" "${pkgdir}/opt/godot/godot-stable-rc-win32-bin.cmd"
	install -Dm755 "${srcdir}/Godot_v${_pkgver}-rc${_pkgverb}_win32_console.exe" "${pkgdir}/opt/godot/godot-stable-rc-win32-bin.exe"
}

