pkgname=(
	"godot4-stable-rc-src"
	##################
	# Godot GDScript #
	##################
	"godot4-stable-rc-bin"
	"godot4-stable-rc-bin32"
	"godot4-stable-rc-export-templates-bin"
	"libgodot4-stable-rc-bin"
	"godot4-stable-rc-web-bin"
	"godot4-stable-rc-osx-bin"
	"godot4-stable-rc-win32-bin"
	"godot4-stable-rc-win64-bin"
	"godot4-stable-rc-android-bin"
	##############
	# Godot Mono #
	##############
	"godot4-stable-rc-mono-bin"
	"godot4-stable-rc-mono-bin32"
	"godot4-stable-rc-mono-export-templates-bin"
	"godot4-stable-rc-mono-osx-bin"
	"godot4-stable-rc-mono-win32-bin"
	"godot4-stable-rc-mono-win64-bin"
	)
_pkgver="4.0.2"
_pkgverb=1
pkgver="${_pkgver}src${_pkgverb}"
pkgrel=1
pkgdesc="Godot4 Game Engine: An advanced, feature packed, multi-platform 2D and 3D game engine."
url="http://www.godotengine.org"
license=("MIT")
groups=(
	"godot-full"
	"godot4"
	"godot40"
	"godot401"
	"godot-bin"
	"godot-src"
)
arch=("any")
_monodepends=(
	"mono>=6.12.0"
	"dotnet-sdk"
	"msbuild"
)
depends=(
	"godot-base"
	"alsa-lib"
	"freetype2"
	"glu"
	"libxcursor"
	"libxinerama"
	"mesa"
)

source=("https://downloads.tuxfamily.org/godotengine/${_pkgver}/rc${_pkgverb}/godot-${_pkgver}-rc${_pkgverb}.tar.xz")
sha512sums=("SKIP")
package_godot4-stable-rc-src(){
	pkgdesc+=" Stable Release Candidate SourceCode"
	groups+=(
		"godot-src"
	)
	install -d "${pkgdir}/opt/godot/src/godot4-stable-rc/"
	cp -r "${srcdir}/godot-${_pkgver}-rc${_pkgverb}/"* "${pkgdir}/opt/godot/src/godot4-stable-rc/"
}

##################
# Godot GDScript #
##################

source+=("https://downloads.tuxfamily.org/godotengine/${_pkgver}/rc${_pkgverb}/Godot_v${_pkgver}-rc${_pkgverb}_linux.x86_64.zip")
sha512sums+=("SKIP")
package_godot4-stable-rc-bin(){
	pkgdesc+=" Stable Release Candidate Binary Version"
	groups+=(
		"godot-gdscript"
		"godot-standard"
		"godot-standard64"
		"godot-64"
	)
	install -Dm755 "${srcdir}/Godot_v${_pkgver}-rc${_pkgverb}_linux.x86_64" "${pkgdir}/opt/godot/godot4-stable-rc-bin"
	
	install -d "${pkgdir}/usr/bin/"
	ln -s "/opt/godot/godot4-stable-rc-bin" "${pkgdir}/usr/bin/godot4-stable-rc-bin"
	
	install -d "${pkgdir}/usr/share/applications/"
	cat > "${pkgdir}/usr/share/applications/godot4-stable-rc-bin.desktop" <<-EOF
		[Desktop Entry]
		Name=Godot4 Engine Stable Release Candidate Binary v${_pkgver}-rc${_pkgverb}
		GenericName="Advanced cross-platform 2D and 3D libre game engine. Stable Release Candidate Binary Version"
		Comment=Application for making games
		Exec=/usr/bin/godot4-stable-rc-bin %U
		Icon=godot
		Terminal=false
		Type=Application
		MimeType=application/x-godot-project;
		Categories=Development;IDE;
	EOF
}

